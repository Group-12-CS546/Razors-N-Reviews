{{!-- <h1>Characters Found</h1> --}}
<main>

    <div class="card-deck">
        <div class="col-sm-6">
            <div class="card" style="width: 18rem;">
                <img class="card-img-top">
                <div class="card-body">
                     <form id="login" action="/salons/{{this.salonId}}/delete" method="POST"> 
        <div class="dropdown">
            <button class="dropbtn">Options</button>
            <div class="dropdown-content">
                {{!-- <p><a href="/reviews/{{this.review._id}}/edit">Edit Review</a></p>
                <p><a href="../sal/{{this.restaurant._id}}/{{this.review._id}}/delete">Delete Review</a></p> 
                {{!-- <p><a href="/salons/{{this.salonId}}/delete">Delete Salon</a></p> --}}
                <p><a href="/salons/{{this.salonId}}/delete">Delete Salon</a></p>
                <p><a href="/salons/{{this.salonId}}/edit">Edit Salon</a></p>
            </div>
        </div>
    </form>
                    <h1>Salon Details</h1>
                    
                    {{!-- <h5 class="card-title"><a href="/salons/{{salonId._id}}">{{this.name}}</a></h5> --}}
                    <p class="card-text bold-id">Name: {{this.name}}</p>
                    <p class="card-text bold-id">Website: {{this.website}}</p>
                    <p class="card-text bold-id">Service: {{this.service}}</p>
                    <p class="card-text bold-id">Address: {{this.address}}</p>
                    <p class="card-text bold-id">City: {{this.city}}</p>
                    <p class="card-text bold-id">State: {{this.state}}</p>
                    <p class="card-text bold-id">Zip: {{this.zip}}</p>
                    <p class="card-text bold-id">Longitude: {{this.longitude}}</p>
                    <p class="card-text bold-id">Latitude: {{this.latitude}}</p>
                    <p class="card-text bold-id">Rating: {{this.rating}}</p>
                    <p class="card-text bold-id">CovidRating: {{this.covidRating}}</p>
                    {{!-- <a href="#" class="btn btn-primary">Go somewhere</a> --}}
                </div>
                
            </div>
        </div>
        
    </div>


    <div class="card-deck">
        <div class="col-sm-6">
            <div class="card" style="width: 18rem;">
                <img class="card-img-top">
                <div class="card-body">
                    <h1>Given Reviews</h1>
                    {{!-- <h5 class="card-title"><a href="/salons/{{salonId._id}}">{{this.name}}</a></h5> --}}
                    {{#each this.getReviews}}
                    {{!-- <p class="card-text">reviewText: {{this._id}}</p> --}}
                    <p class="card-text">reviewText: {{this.reviewText}}</p>
                    <p class="card-text">rating: {{this.rating}}</p>
                    <p class="card-text">covidRating: {{this.covidRating}}</p>
                    <p><a href="/reviews/reviews/updatereview/{{this._id}}">Edit Review</a></p>
                    <p><a href="/reviews/reviews/deletereview/{{this._id}}">Delete Review</a></p>

                    <div class="card-text">
                        <p class="card-text">Comments</p>
                        {{#each this.commentTexts}}
                        <p>{{this}}</p>
                        {{/each}}
                        <div class="center">
                            <h4>Post a comment</h4>
                            <form action="/comments/add/{{this._id}}" method="POST" class="new-comment-form">
                                <div class="form-group">
                                    <label for="commentText">Comment:</label>
                                    <input type="text" class="form-control" id="commentText" name="commentText"
                                        aria-describedby="commentText" placeholder="Enter a comment....">

                                </div>
                                <button type="submit">Submit</button>
                            </form>
                        </div>
                        <div>
                            <h1>Covid Review</h1>
                            <form action="/covidReviews/{{this._id}}/add" method="POST" class="new-covidReview-form">
                                <div class="form-group">
                                    <label for="Parameter1">Hygiene:</label>
                                    <input type="text" class="form-control" id="Parameter1" name="Parameter1"
                                        aria-describedby="Parameter1" placeholder="Enter a value between 1 and 5">

                                </div>
                                <div class="form-group">
                                    <label for="Parameter2">Cleanliness:</label>
                                    <input type="text" class="form-control" id="Parameter2" name="Parameter2"
                                        aria-describedby="Parameter2" placeholder="Enter a value between 1 and 5">

                                </div>
                                <div class="form-group">
                                    <label for="Parameter3">Everybody Wearing a mask:</label>
                                    <input type="text" class="form-control" id="Parameter3" name="Parameter3"
                                        aria-describedby="Parameter3" placeholder="Enter a value between 1 and 5">

                                </div>
                                <div class="form-group">
                                    <label for="Parameter4">Sanitation:</label>
                                    <input type="text" class="form-control" id="Parameter4" name="Parameter4"
                                        aria-describedby="Parameter4" placeholder="Enter a value between 1 and 5">

                                </div>
                                <div class="form-group">
                                    <label for="Parameter5">Social Distancing:</label>
                                    <input type="text" class="form-control" id="Parameter5" name="Parameter5"
                                        aria-describedby="Parameter5" placeholder="Enter a value between 1 and 5">

                                </div>
                                <button type="submit">Submit</button>
                            </form>
                        </div>
                    </div>
                    {{/each}}
                    {{!-- <a href="#" class="btn btn-primary">Go somewhere</a> --}}
                </div>
            </div>
        </div>
    </div>

    {{!-- <div class="dblclick container">
        {{#if (checkListMem ../customer._id this.likes)}}
        <i class="likebtn fa fa-thumbs-up filled" data-rid="{{this.id}}" data-uid="{{../customer._id}}"></i>
        {{else}}
        <i class="likebtn fa fa-thumbs-up" data-rid="{{this.id}}" data-uid="{{../customer._id}}"></i>
        {{/if}}
        <span class="like">{{this.like.length}}</span>
        {{#if (checkListMem ../customer._id this.dislikes)}}
