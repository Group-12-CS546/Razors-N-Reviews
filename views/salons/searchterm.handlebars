<h1>Characters Found</h1>
<main>
    {{!-- <h2>{{data.name}}</h2> --}}
    {{!-- <h1>{{this.data}}</h1> --}}
    {{#each data}}
    <span>{{this.name}}</span>
    <span>{{this.address}}</span>
    <span>{{this.city}}</span>
    <span>{{this.rating}}</span>
    {{/each}}
    {{#if data}}
    <ul>
        {{#each data}}
        <li>
            <a style="color: white;" href="/salons/">******{{this.data}}</a>
        </li>
        {{/each}}
    </ul>
    {{else}}
    <p class="not-found">We're sorry, but no results were found for {{this.data.name}}.</p>
    {{/if}}


    {{#if hasErrors}}
    {{#each errors}}
    <li>
        {{this}}
    </li>
    <div class="another">
        <a href="/">Make another search</a>
    </div>
    {{/each}}
    {{/if}}
    <div class="another">
        <a href="/">Make another search</a>
    </div>

</main>